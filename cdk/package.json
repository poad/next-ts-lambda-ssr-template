{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "eslint-check": "eslint './{bin,lib}/**/*.ts'",
    "eslint-fix": "eslint --fix './{bin,lib}/**/*.ts'",
    "prettier-check": "prettier --check bin lib",
    "prettier-format": "prettier --write bin lib",
    "lint": "bun prettier-check && bun eslint-check",
    "lint-fix": "bun prettier-format && bun eslint-fix"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "aws-cdk": "^2.139.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.139.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
