{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "eslint-check": "eslint --ext .ts bin lib",
    "eslint-fix": "eslint --fix --ext .ts bin lib",
    "prettier-check": "prettier --check bin lib",
    "prettier-format": "prettier --write bin lib",
    "lint": "bun prettier-check && bun eslint-check",
    "lint-fix": "bun prettier-format && bun eslint-fix"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "aws-cdk": "^2.110.1",
    "esbuild": "^0.19.7",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.110.1-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.110.1-alpha.0",
    "aws-cdk-lib": "^2.110.1",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
